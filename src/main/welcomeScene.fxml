<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<StackPane alignment="CENTER" prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.WelcomeSceneController">
    <children>
        <ImageView fx:id="image" fitHeight="800" fitWidth="1200">
         <image>
            <Image url="@images/welcome_background.jpg" />
         </image></ImageView>
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="50.0" GridPane.columnIndex="2" GridPane.rowIndex="4">
            <children>
                <Button fx:id="startButton" alignment="CENTER" mnemonicParsing="false" onAction="#handleStartButton" prefHeight="75.0" prefWidth="150.0" style="-fx-background-color: #18a734;" text="Start" textFill="WHITE">
                    <font>
                        <Font size="28.0" />
                    </font>
                    <HBox.margin>
                        <Insets />
                    </HBox.margin>
                </Button>
                <Button fx:id="quitButton" alignment="CENTER" mnemonicParsing="false" onAction="#handleQuitButton" prefHeight="75.0" prefWidth="150.0" style="-fx-background-color: #18a734;" text="Quit" textAlignment="CENTER" textFill="WHITE">
                    <font>
                        <Font size="28.0" />
                    </font>
                </Button>
            </children>
            <opaqueInsets>
                <Insets />
            </opaqueInsets>
         <padding>
            <Insets left="20.0" top="75.0" />
         </padding>
        </HBox>
    </children>
    <opaqueInsets>
        <Insets />
    </opaqueInsets>
</StackPane>
