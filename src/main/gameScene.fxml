<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<StackPane alignment="CENTER" prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.GameSceneController">
    <GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0" prefWidth="1200.0">
        <columnConstraints>
            <ColumnConstraints maxWidth="1100.0" minWidth="1100.0" prefWidth="1100.0" />
            <ColumnConstraints maxWidth="100.0" minWidth="100.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints maxHeight="-Infinity" minHeight="800.0" prefHeight="800.0" />
        </rowConstraints>
        <children>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="8.0" GridPane.columnIndex="0" GridPane.rowIndex="0">
                <children>
                    <Label text="Game Here" textFill="#469021">
                        <font>
                            <Font size="22.0" />
                        </font>
                        <VBox.margin>
                            <Insets left="150.0" />
                        </VBox.margin>
                    </Label>
                </children>
            </VBox>
            <VBox alignment="BOTTOM_CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0" GridPane.columnIndex="1" GridPane.rowIndex="0">
                <children>
                    <Button fx:id="pauseButton" alignment="CENTER" mnemonicParsing="false" onAction="#handlePauseButton" prefHeight="30.0" prefWidth="70.0" style="-fx-background-color: #18a734;" text="Pause" textFill="WHITE">
                        <font>
                            <Font name="Chalkduster" size="14.0" />
                        </font>
                    </Button>
                    <Button fx:id="quitButton" alignment="CENTER" mnemonicParsing="false" onAction="#handleQuitButton" prefHeight="30.0" prefWidth="70.0" style="-fx-background-color: #18a734;" text="Quit" textAlignment="CENTER" textFill="WHITE">
                        <font>
                            <Font name="Chalkduster" size="14.0" />
                        </font>
                    </Button>
                </children>
            <padding>
               <Insets bottom="20.0" />
            </padding>
            </VBox>
        </children>
    </GridPane>
</StackPane>
